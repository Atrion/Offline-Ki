            AgeInfo = ptAgeInfoStruct()
            AgeInfo.setAgeFilename('Elodea')
            AgeInfo.setAgeInstanceName('Elodea')
            Link = ptAgeLinkStruct()
            Link.setAgeInfo(AgeInfo)
            Link.setLinkingRules(PtLinkingRules.kOriginalBook)
            Link.setSpawnPoint(ptSpawnPointInfo('LinkInPointDefault', SpawnPunkt))
            LinkManager = ptNetLinkingMgr()
            LinkManager.linkToAge(Link)